show_expected_result = False
show_hints = False

def add_product_code(product_code, product_id):
    if product_id[4:9] == product_code:
        return product_id
    else:
        first = product_id[:3]
        last = product_id[3:]
        return (f"{first}-{product_code}-{last}") 
